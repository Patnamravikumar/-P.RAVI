import java.util.*;
class Main{
    public static void main(String[]args){
        int[] a={1,2,3,4,5};
        int[] b={1,5,3,7};
        System.out.println(isSorted(a));
        System.out.println(isSorted(b));
    }
    public static boolean isSorted(int[] arr){
        for(int i=0;i<arr.length-1;i++){
            if(arr[i] >arr[i+1]){
                return false;
        }
    }
    return true;
}
}
