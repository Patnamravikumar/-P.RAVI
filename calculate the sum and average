import java.util.*;
class Main{
    public static void main(String[]args){
        int[] a={10,20,30,40};
        int sum=0;
        double average;
        for(int number : a){
            sum += number;
        }
        average = (double)sum/a.length;
        System.out.println("SUM:" +sum);
        System.out.println("avg:" +average);
    }
}
