import java.util.*;
class Main{
    public static void main(String[]args){
        int[] a={1,2,3,4,5,6};
        int evenCount=0;
        int oddCount=0;
        for(int i=0;i< a.length;i++){
            if(a[i] % 2 == 0){
                evenCount++;
            }else{
                oddCount++;
            }
        }
        System.out.println("evenoccur:" + evenCount);
        System.out.println("oddoccur:" + oddCount);
    }
}
